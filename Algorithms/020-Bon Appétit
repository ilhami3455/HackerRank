def bonAppetit(bill, k, b):
    toplam=sum(bill)//2
    a=bill.pop(k)
    if sum(bill)//2==b:
        print("Bon Appetit")
    else:
        print(b-(sum(bill)//2))
nk = input().rstrip().split()
n = int(nk[0])
k = int(nk[1])
bill = list(map(int, input().rstrip().split()))
b = int(input().strip())
bonAppetit(bill, k, b)
