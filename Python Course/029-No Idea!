nm=input().split()
n=nm[0]
m=nm[1]
arr = list(map(int, input().rstrip().split()))
A = set(map(int, input().rstrip().split()))
B = set(map(int, input().rstrip().split()))
happiness=0
for i in arr:
    if i in A:
        happiness+=1
    elif i in B:
        happiness-=1
    else:
        pass
print(happiness)
